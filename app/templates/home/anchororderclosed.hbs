<div class="content-wrapper">
  <!-- Content Header (Page header) -->
  <section class="content-header">
    <h1>Anchor Order Closed</h1>
    <ol class="breadcrumb">
      <li><a href="#"><i class="fa fa-dashboard"></i> Level</a></li>
      <li class="active">Here</li>
    </ol>
  </section>

  <!-- Main content -->
  <section class="content">

    <!-- Your Page Content Here -->
<div class="row">
      <div class="col-md-6">
        <!-- small box -->
        <div class="smallbox_bgaqua">
          <div class="inner">
            <h3>150</h3>

            <p>Number of Anchor Programs</p>
          </div>
        </div>
      </div>
      <!-- ./col -->
     
      <div class="col-md-6">
        <!-- small box -->
        <div class="smallbox_bggreen">
          <div class="inner">
            <h3>53<sup style="font-size: 20px">%</sup></h3>

            <p>Programs Completed</p>
          </div>
         </div>
      </div>
      <!-- ./col -->

</div> 

<div class="row">
      <div class="col-md-6">
        <!-- small box -->
        <div class="smallbox_bgyellow">
          <div class="inner">
            <h3>44</h3>

            <p>Payments made to Vendors</p>
          </div>
          </div>
      </div>
      <!-- ./col -->
      <div class="col-md-6">
        <!-- small box -->
        <div class="smallbox_bgred">
          <div class="inner">
            <h3>65</h3>
            <p>Interest/Penal amount changed</p>
          </div>
         </div>
      </div>
</div>

<div class="row">
    <div class="col-md-6">
        <!-- small box -->
        <div class="smallbox_bgaqua">
          <div class="inner">
            <h3>150</h3>

            <p>Average Settlement Time</p>
          </div>
        </div>
      </div>
      <div class="col-md-6">
        <!-- small box -->
        <div class="smallbox_bggreen">
          <div class="inner">
            <h3>53<sup style="font-size: 20px">%</sup></h3>

            <p>Programs Completed</p>
          </div>
          
        </div>
      </div>
    </div>
    <!-- Default box -->

    <div class="box">
      <div class="box-header">
        <h3 class="box-title">Anchor Orders</h3>

        <div class="box-tools">
          <ul class="nav nav-tabs" style="border: none;">
            <li class="dropdown">
            {{#link-to 'home.settlement'}} Sort By  {{/link-to}}
              </li>
            <li class="pull-right">
              <div class="input-group input-group-sm" style="width: 250px;">
                {{input type="text" name="table_search" class="form-control pull-right"
                placeholder="Search by Anchor Program Id"}}

                <div class="input-group-btn">
                  <button type="submit" class="btn btn-default"></button>
                </div>
              </div>
            </li>
          </ul>

        </div>
      </div>
      </div>
      <!-- /.box-header -->
    <div class="box-body no-padding">
        <table class="table" name="anchororder">
          <tr>
            <th>Anchor Program Id</th>
            <th>Customer Name</th>
            <th>Vendor Name</th>
            <th>Stage</th>
            <th>Pending payments</th>
            <th>Received payments</th>
            <th>Status</th>
          </tr>
           <tr>
            <td>{{item.anchorprogramID}}</td>
            <td>{{item.anchorname}}</td>
            <td>{{item.vendorfname}}</td>
            <td>{{item.stage}}</td>
            <td>{{item.amountpending}}</td>
            <td>{{item.amountreceived}}</td>
            <td>{{item.status}}</td>
          </tr>
        </table>
      </div>
    
  </section>
  <!-- /.content -->
</div>
